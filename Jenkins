 pipeline {
      agent any
      
          tools{
        maven 'maven3.8.6'
    }
    stages{
        stage('git clone'){
            steps{
                git 'https://github.com/Maruthiraok/maven-web-application.git'
            }
        }
        stage('Maven clean'){
            steps{
               sh "mvn clean Install"
            }
        }
        stage('Build Sucess'){
            steps{
               echo "Build sucessfull"
            }
        }
    }
    
}
